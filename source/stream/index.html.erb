<body>
   <div class="hello">
      <div class="home-link"><a href="/#thank-you" title="Return to homepage">H</a>
      </div><div class="highlight">Hello! This is my social stream <span class="carret">&#9646;</span></div>
      <br/>
   </div>
   <div id="lifestream">
      <p class='ie'>Hold on, please. If it's taking too long, it means your browser is not supported
        or you don't have JavaScript enabled.<br/>You can still check what I'm doing on
        <a href="http://dribbble.com/davidfrancisco">Dribbble</a>,
        <a href="http://github.com/dmfrancisco">Github</a>,
        <a href="http://slideshare.net/dmfrancisco">Slideshare</a>,
        <a href="http://blog.dmfranc.com/">Tumblr</a>,
        <a href="http://twitter.com/dmfrancisco">Twitter</a>,
        <a href="http://vimeo.com/davidfrancisco">Vimeo</a>,
        <a href="http://last.fm/user/ElGangoFeo">Last.fm</a> and
        <a href="http://stackoverflow.com/users/543293/davidfrancisco">Stack Overflow</a>.
      </p>
   </div>
   <footer>Made with the <a href="http://git.io/cLetpQ">jQuery Lifestream</a> library. Icons adapted from the work of
      <a href="http://octo.cat/8d">design deck</a> and <a href="http://octo.cat/8e">Yusuke K</a>.</footer>

   <script src="//ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js"></script>
   <script>window.jQuery || document.write('<script src="../js/libs/jquery-1.7.2.min.js"><\/script>')</script>
   <script src="../js/libs/jquery.timeago.lifestream.min.js"></script>
   <script>
     (function(){
       function isIE() {
         return /msie/i.test(navigator.userAgent) && !/opera/i.test(navigator.userAgent);
       }

       var count = 0,
       list = [
         {
           service: 'dribbble',
           user: 'davidfrancisco'
         },
         {
           service: 'github',
           user: 'dmfrancisco'
         },
         {
           service: 'slideshare',
           user: 'dmfrancisco'
         },
         {
           service: 'tumblr',
           user: 'davidfrancisco'
         },
         {
           service: 'twitter',
           user: 'dmfrancisco'
         },
         {
           service: 'vimeo',
           user: 'davidfrancisco'
         },
         {
           service: 'lastfm',
           user: 'elgangofeo'
         },
         {
           service: 'stackoverflow',
           user: '543293'
         }
         // {
         //   service: 'forrst',
         //   user: 'davidfrancisco'
         // }
       ];

       Date.prototype.toISO8601 = function(date) {
           var pad = function (amount, width) {
               var padding = "";
               while (padding.length < width - 1 && amount < Math.pow(10, width - padding.length - 1))
                   padding += "0";
               return padding + amount.toString();
           }
           date = date ? date : new Date();
           var offset = date.getTimezoneOffset();
           return pad(date.getFullYear(), 4)
               + "-" + pad(date.getMonth() + 1, 2)
               + "-" + pad(date.getDate(), 2)
               + "T" + pad(date.getHours(), 2)
               + ":" + pad(date.getMinutes(), 2)
               + ":" + pad(date.getUTCSeconds(), 2)
               + (offset > 0 ? "-" : "+")
               + pad(Math.floor(Math.abs(offset) / 60), 2)
               + ":" + pad(Math.abs(offset) % 60, 2);
       };

       if (!isIE()) {
         $("#lifestream").lifestream({
           limit: 200,
           defaultServiceLimit: 70,
           serviceLimits: {
             lastfm: 10
           },
           list: list,
           feedloaded: function(){
             count++;
             // Check if all the feeds have been loaded
             if( count === list.length ){
               $("#lifestream li").each(function(){
                 var element = $(this),
                     date = new Date(element.data("time"));
                 element.append(' &nbsp; <abbr class="timeago" title="' + date.toISO8601(date) + '">' + date + "</abbr>");
               })
               $("#lifestream .timeago").timeago();
             }
           }
         });
       }

       function blink(selector) {
         $(selector).fadeOut('slow', function() {
           $(this).fadeIn('slow', function() {
             blink(this);
           });
         });
       }
       blink(".carret");
     })();
   </script>
</body>
</html>