     }
     #lifestream a {
       color: #f6dd97;
       text-decoration: none;
     }
     #lifestream a:hover {
       color: #dadddd;
       text-decoration: none;
       border-bottom: 1px dotted #dadddd;
     }
     abbr[title] {
       border-bottom-width: 0;
     }
     abbr[title]:hover {
       border-bottom-width: 1px;
     }
     #lifestream .lifestream-dribbble      { background-image: url(data:image/png;base64,);}
     #lifestream .lifestream-github        { background-image: url(data:image/png;base64,);}
     #lifestream .lifestream-lastfm        { background-image: url(data:image/png;base64,);}
     #lifestream .lifestream-slideshare    { background-image: url(data:image/png;base64,);}
     #lifestream .lifestream-stackoverflow { background-image: url(data:image/png;base64,);}
     #lifestream .lifestream-tumblr        { background-image: url(data:image/png;base64,);}
     #lifestream .lifestream-twitter       { background-image: url(data:image/png;base64,);}
     #lifestream .lifestream-vimeo         { background-image: url(data:image/png;base64,);}
     #lifestream .lifestream-twitter       a { color: #50E0E1; }
     #lifestream .lifestream-tumblr        a { color: #80AAC6; }
